{
  "schemaVersion": 1,
  "id": "fowlplay",
  "version": "${version}",
  "name": "Fowl Play",
  "description": "Just smile and wave, boys. Smile and wave.",
  "authors": [
    "aqario"
  ],
  "contributors": [
    "Penguin60",
    "Disketaa",
    "owen233666",
    "sanduicheirainox",
    "Gnom1112"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/fowlplay",
    "sources": "https://github.com/aqariio/Fowl-Play",
    "issues": "https://github.com/aqariio/Fowl-Play/issues"
  },
  "license": "MIT",
  "icon": "assets/fowlplay/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "aqario.fowlplay.core.fabric.FowlPlayFabric"
    ],
    "client": [
      "aqario.fowlplay.client.fabric.FowlPlayFabricClient"
    ],
    "fabric-datagen": [
      "aqario.fowlplay.datagen.FowlPlayDataGen"
    ],
    "modmenu": [
      "aqario.fowlplay.common.integration.fabric.ModMenuIntegration"
    ]
  },
  "mixins": [
    "fowlplay.common.mixins.json",
    "fowlplay.fabric.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.21",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "architectury": ">=${architectury_version}",
    "smartbrainlib": ">=${sbl_version}",
    "yet_another_config_lib_v3": ">=${yacl_version}"
  },
  "custom": {
    "mc-publish": {
      "dependencies": [
        "architectury-api(required)",
        "yacl(required)"
      ]
    }
  }
}